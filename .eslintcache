  height: 100%;\r\n    background-color: black;\r\n`;\r\n\r\nconst H1 = styled(StyledH1)`\r\n    opacity: 0;\r\n    color: hsl(200, 100%, 50%);\r\n    text-shadow: 2px 2px 10px black;\r\n    animation: ${direction(\"opacity: 0\", \"opacity: 1\")} 3s 3s linear forwards;\r\n`;\r\n\r\nconst Phrase = styled(StyledPhrase)`\r\n    opacity: 0;\r\n    color: hsl(200, 100%, 60%);\r\n    animation: ${direction(\"opacity: 0\", \"opacity: 1\")} 3s 4s linear forwards;\r\n`;\r\n\r\nconst Logo = styled(StyledImg)`\r\n    opacity: 0;\r\n    animation: ${direction(\"opacity: 0\", \"opacity: 1\")} 3s 5s linear forwards;\r\n`;\r\n\r\n// SETTINGS /////////////////////////////////////////////////////////////\r\nconst maxFocusableElements = 1;\r\n\r\nexport default function SpashIntro({width, triggerExit}) {\r\n\r\n    // FOCUS and ENTERSELECT /////////////////////////////////////////////////\r\n    const [tabIndex, setTabIndex] = useState(0);\r\n\r\n    // 'useKey' returns true if key is held down.\r\n    const tab = useKey(\"Tab\");\r\n    const enter = useKey(\"Enter\");\r\n\r\n    useEffect(()=> {\r\n        if (tab) {\r\n            if (tabIndex === maxFocusableElements) {\r\n                setTabIndex(1);\r\n            }\r\n            else {\r\n                setTabIndex(tabIndex + 1);\r\n            };\r\n        };\r\n    },[tab]);\r\n\r\n    // RENDER /////////////////////////////////////////////////////////\r\n\r\n    return (\r\n        <LayerDiv>\r\n            <RothkoDiv2 \r\n                spatial={[0, 0, 20, 19]} \r\n                width={width}\r\n                color={\"hsl(200, 100%, 50%)\"} \r\n            />\r\n            \r\n            <header>\r\n                <EnterButton \r\n                    spatial={[3, 3, 14, 10]} \r\n                    width={width}\r\n                    highlight={null}\r\n                    colors={[\r\n                        \"hsl(15, 100%, 60%)\",\r\n                        \"blue\",\r\n                        \"hsl(15, 80%, 40%)\",\r\n                        \"aqua\",\r\n                        \"white\",\r\n                    ]}\r\n                    focus={tabIndex === 1}\r\n                    enterSelect={enter}\r\n                    linkTo={\"./home\"}\r\n                    triggerExit={triggerExit}\r\n                >\r\n                    ENTER\r\n                </EnterButton>\r\n\r\n                <H1 spatial={[0, 15, 20, 2]} width={width}>Rothko</H1>\r\n\r\n                <Phrase\r\n                    width={width}\r\n                    spatial={[0, 18, 20, 7]}\r\n                >\r\n                    an<br/>\r\n                    audio / visual<br/>\r\n                    web design theme<br/>\r\n                    by<br/>\r\n                </Phrase> \r\n\r\n                <Logo src={logo} alt=\"logo of web design company Dreeko\" \r\n                    spatial={[0, 26, 20, 2]}\r\n                />\r\n            </header>\r\n        </LayerDiv>\r\n    );\r\n};\r\n\r\n","C:\\Users\\DoubleD\\Desktop\\Code\\2020\\020 Rothko Web Theme\\src\\App\\Content\\1 Pages\\SplashScreen.js",[],"C:\\Users\\DoubleD\\Desktop\\Code\\2020\\020 Rothko Web Theme\\src\\App\\Content\\2 Layers\\Gutter.js",[],"C:\\Users\\DoubleD\\Desktop\\Code\\2020\\020 Rothko Web Theme\\src\\App\\Content\\4 Styling\\RothkoDivs\\RothkoDiv1.js",[],"C:\\Users\\DoubleD\\Desktop\\Code\\2020\\020 Rothko Web Theme\\src\\App\\Content\\4 Styling\\helpers\\positioning.js",[],"C:\\Users\\DoubleD\\Desktop\\Code\\2020\\020 Rothko Web Theme\\src\\App\\Content\\4 Styling\\RothkoDivs\\RothkoDiv2.js",[],"C:\\Users\\DoubleD\\Desktop\\Code\\2020\\020 Rothko Web Theme\\src\\App\\Content\\4 Styling\\Headers\\StyledH1.js",[],"C:\\Users\\DoubleD\\Desktop\\Code\\2020\\020 Rothko Web Theme\\src\\App\\Content\\4 Styling\\Text\\StyledPhrase.js",[],"C:\\Users\\DoubleD\\Desktop\\Code\\2020\\020 Rothko Web Theme\\src\\App\\Content\\4 Styling\\helpers\\direction.js",[],"C:\\Users\\DoubleD\\Desktop\\Code\\2020\\020 Rothko W